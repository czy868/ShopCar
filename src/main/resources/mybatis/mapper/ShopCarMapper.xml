<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zy.mapper.ShopCarMapper">
    <insert id="insertsopcar" parameterType="Car">
        insert into car(singleNumber,applicationSector,applicationData,applicationName,productName,specification,quantity,address,needsTime,total)
        values(#{singleNumber},#{applicationSector},#{applicationData},#{applicationName},#{productName},#{specification},#{quantity},#{address},#{needsTime},#{total})
    </insert>
</mapper>
